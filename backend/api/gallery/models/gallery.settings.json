{
  "kind": "collectionType",
  "collectionName": "galleries",
  "info": {
    "name": "gallery"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "link": {
      "type": "string",
      "required": true,
      "default": "/"
    }
  }
}
